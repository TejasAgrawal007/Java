
// 1. Extending the Thread Class
class Main {
    public static void main(String[] args) {
       Threading t1 = new Threading();
       t1.getData();
    }
}


class Threading extends Thread
{
    void getData()
    {
        for(int i = 1; i <= 10; i++)
        {
            System.out.print(i + " ");
        }
    }
}
