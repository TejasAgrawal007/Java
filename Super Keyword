
class Main {
    public static void main(String[] args) {
        Programmer p1 = new Programmer();
        p1.showName();
    }
}

class Company
{
    String name = "Disha Academy";
}


class Programmer extends Company
{
    String name = "Tejas Agrawal";
        
    void showName()
    {
         System.out.println(name);    
         System.out.println(super.name);  
    }
    
}




